{
    "name": "Extensor",
    "version": "1.0",
    "description": "cmd+k",
    "permissions": [
        "bookmarks"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "js": [
            "shortcut.js",
            "page.js",
            "sync.js",
            "utils.js"
        ],
        "css": [
            "magic.css"
        ],
        "matches": [
            "http://*/*",
            "https://*/*"
        ]
    }],
    "manifest_version": 2
}